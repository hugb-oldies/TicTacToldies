package is.ru.tictactoe;

import java.util.*;
import java.util.regex.Matcher;


public class Board{

	private static board = new int[3][3];

	public Board () {
		//board = new int[3][3]; // { {0, 0, 0 }, {0, 0, 0 }, {0, 0, 0 } } ;
	}

	public static int checkSquare(int x, int y) {
		return board[x][y];
	}
}


